{
   "background": {
      "persistent": false,
      "scripts": [ "init.js" ]
   },
   "browser_action": {
      "default_title": "Bookmarks Tab"
   },
   "chrome_url_overrides": {
      "newtab": "bookmarks.html"
   },
   "description": "Replace new tab page with an overview of your bookmarks",
    "icons": {
      "128": "icon.png",
      "16": "icon.png"
   },
   "manifest_version": 2,
   "name": "Bookmarks Tab",
   "permissions": [ "bookmarks"],
   "version": "0.0.4"
}

