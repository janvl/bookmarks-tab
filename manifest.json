{
   "background": {
      "persistent": false,
      "scripts": [ "init.js" ]
   },
   "browser_action": {
      "default_title": "Bookmarks Tab"
   },
   "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
   "description": "Replace new tab page with an overview of your bookmarks",
    "icons": {
      "128": "icon.png",
      "16": "icon.png"
   },
   "manifest_version": 2,
   "name": "Bookmarks Tab",
   "options_page": "options.html",
   "permissions": [ "bookmarks", "tabs"],
   "version": "0.0.4"
}

